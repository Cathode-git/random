{
  "name": "app",
  "version": "0.1.0",
  "description": "app",
  "main": "src/es6-init.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "clean": "rm -rf .cache",
    "compile": "cross-env NODE_ENV=production electron-compile --appDir . ./src",
    "package-this": "rm -rf .cache && electron-compile --appDir . ./src && cross-env NODE_ENV=production electron-packager . app --out=dist --overwrite",
    "package": "run-s clean compile package-linux package-win",
    "package-linux": "cross-env NODE_ENV=production electron-packager . app --out=dist --platform=linux --arch=all --overwrite",
    "package-win": "cross-env NODE_ENV=production electron-packager . app --out=dist --platform=win32 --arch=all --overwrite"
  },
  "devDependencies": {
    "cross-env": "^2.0.1",
    "electron": "^1.3.5",
    "electron-compilers": "^3.1.6",
    "electron-packager": "^8.0.0",
    "npm-run-all": "^3.1.0"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2016-node5": "^1.1.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "electron-compile": "^3.3.5",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  }
}
